{
  "name": "convenient-whitelist",
  "version": "1.0.0",
  "description": "This application allows admins and moderator to easily manage a whitelist for their Minecraft server. Users can be added and removed from the whitelist by simply clicking on a corresponding button in the admin panel.",
  "main": "index.js",
  "scripts": {
    "build": "esbuild --bundle src/index.ts --outfile=dist/index.js --platform=node --target=node14 --format=cjs",
    "start": "node dist/index.js",
    "watch": "esbuild --watch --bundle src/index.ts --outfile=dist/index.js --platform=node --target=node14 --format=cjs",
    "dev": "concurrently \"npm:watch\" \"npm:start\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "commonjs",
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "minecraft-protocol": "^1.41.2",
    "pg": "^8.10.0",
    "prisma": "^4.13.0",
    "rcon": "^1.1.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.18",
    "prisma-client": "^0.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
