# Connection to Backend Service
REACT_APP_API_URL="http://localhost:3001"

# Connection to MC-Server Sleeper Status Service
REACT_APP_STATUS_API_URL="http://localhost:3000"

REACT_APP_SERVER_STATUS_URL="http://localhost:3000"

# Redirect to Bedrock Minecraft Server
REACT_APP_SERVER_NAME="localhost"
REACT_APP_SERVER_IP="localhost"
REACT_APP_SERVER_PORT="19132"

# Disable ESLint plugin in react-scripts (required for ESLint 9 compatibility)
# The ESLint plugin in react-scripts is only compatible with ESLint 8
# We use the standalone ESLint CLI for linting instead
DISABLE_ESLINT_PLUGIN=true